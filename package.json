{
  "name": "estore",
  "version": "1.0.0",
  "description": "This is a example project about Authentication",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon --inspect src/index.js",
    "watch": "node-sass --watch src/resources/scss/ --output src/public/css/",
    "beautiful": "prettier --single-quote --tab-width 4 --trailing-comma all --write src/**/*.{js,json,scss}",
    "format-code": "lint-staged",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gnohP18/EStore.git"
  },
  "keywords": [
    "Learn",
    "nodejs",
    "estore",
    "estore",
    "nodejs"
  ],
  "author": "gnohp18",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gnohP18/EStore/issues"
  },
  "homepage": "https://github.com/gnohP18/EStore#readme",
  "lint-staged": {
    "src/**/*.{js,json,scss}": "prettier --single-quote --tab-width 4 --trailing-comma all --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier --single-quote --tab-width 4 --trailing-comma all --write src/**/*.{js,json,scss}"
    }
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-handlebars": "^7.1.0"
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "morgan": "^1.10.0",
    "node-sass": "^9.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0"
  }
